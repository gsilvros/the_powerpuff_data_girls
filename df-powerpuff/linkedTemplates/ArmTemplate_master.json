{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-powerpuff"
		},
		"dl_powerpuff_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://thepowerpuffdatagirls.dfs.core.windows.net/"
		},
		"dl_sus_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://susbecantt.dfs.core.windows.net/"
		},
		"kv_powerpuff_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-powerpuff.vault.azure.net/"
		},
		"trigger_ambulatorio_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d44271f6-fec8-44b4-b79f-a0fd742c7602/resourceGroups/susbecantt/providers/Microsoft.Storage/storageAccounts/susbecantt"
		},
		"trigger_hospitalar_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d44271f6-fec8-44b4-b79f-a0fd742c7602/resourceGroups/susbecantt/providers/Microsoft.Storage/storageAccounts/susbecantt"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-powerpuff_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"kv_powerpuff_properties_typeProperties_baseUrl": {
						"value": "[parameters('kv_powerpuff_properties_typeProperties_baseUrl')]"
					},
					"dl_powerpuff_properties_typeProperties_url": {
						"value": "[parameters('dl_powerpuff_properties_typeProperties_url')]"
					},
					"dl_sus_properties_typeProperties_url": {
						"value": "[parameters('dl_sus_properties_typeProperties_url')]"
					},
					"trigger_ambulatorio_properties_typeProperties_scope": {
						"value": "[parameters('trigger_ambulatorio_properties_typeProperties_scope')]"
					},
					"trigger_hospitalar_properties_typeProperties_scope": {
						"value": "[parameters('trigger_hospitalar_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}