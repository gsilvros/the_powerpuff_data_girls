{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-powerpuff"
		},
		"kv_powerpuff_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-powerpuff.vault.azure.net/"
		},
		"dl_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0811-140226-5u6jowjc"
		},
		"dl_powerpuff_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://thepowerpuffdatagirls.dfs.core.windows.net/"
		},
		"dl_sus_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://susbecantt.dfs.core.windows.net/"
		},
		"trigger_ambulatorio_properties_bronze_ambulatorio_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"trigger_ambulatorio_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d44271f6-fec8-44b4-b79f-a0fd742c7602/resourceGroups/susbecantt/providers/Microsoft.Storage/storageAccounts/susbecantt"
		},
		"trigger_hospitalar_properties_notebook_bronze_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"trigger_hospitalar_properties_bronze_hospitalar_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"trigger_hospitalar_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d44271f6-fec8-44b4-b79f-a0fd742c7602/resourceGroups/susbecantt/providers/Microsoft.Storage/storageAccounts/susbecantt"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/kv_powerpuff')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('kv_powerpuff_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/dl_databricks')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDatabricks",
				"typeProperties": {
					"domain": "https://adb-247973449430578.18.azuredatabricks.net",
					"accessToken": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "kv_powerpuff",
							"type": "LinkedServiceReference"
						},
						"secretName": "kv-databricks"
					},
					"existingClusterId": "[parameters('dl_databricks_properties_typeProperties_existingClusterId')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/kv_powerpuff')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dl_powerpuff')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('dl_powerpuff_properties_typeProperties_url')]",
					"accountKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "kv_powerpuff",
							"type": "LinkedServiceReference"
						},
						"secretName": "token-powerpuff"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/kv_powerpuff')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dl_sus')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('dl_sus_properties_typeProperties_url')]",
					"accountKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "kv_powerpuff",
							"type": "LinkedServiceReference"
						},
						"secretName": "token-sus"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/kv_powerpuff')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/notebook_bronze')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Notebook_bronze",
						"type": "DatabricksNotebook",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Users/gsilvros@emeal.nttdata.com/Projeto_integrado/bronze_ingestion",
							"baseParameters": {
								"filename": {
									"value": "@pipeline().parameters.filename",
									"type": "Expression"
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "dl_databricks",
							"type": "LinkedServiceReference"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"filename": {
						"type": "string"
					}
				},
				"variables": {
					"filename": {
						"type": "String"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/dl_databricks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dataset_binario_destino')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "dl_powerpuff",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"container": {
						"type": "string"
					},
					"folder": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "Binary",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"folderPath": {
							"value": "@dataset().folder",
							"type": "Expression"
						},
						"fileSystem": {
							"value": "@dataset().container",
							"type": "Expression"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/dl_powerpuff')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dataset_binario_origem')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "dl_sus",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"container": {
						"type": "string"
					},
					"folder": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "Binary",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"folderPath": {
							"value": "@dataset().folder",
							"type": "Expression"
						},
						"fileSystem": {
							"value": "@dataset().container",
							"type": "Expression"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/dl_sus')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/bronze_ambulatorio')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_ambulatorio",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "BinarySource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"wildcardFolderPath": "SIASUS",
									"wildcardFileName": "*",
									"deleteFilesAfterCompletion": false
								},
								"formatSettings": {
									"type": "BinaryReadSettings"
								}
							},
							"sink": {
								"type": "BinarySink",
								"storeSettings": {
									"type": "AzureBlobFSWriteSettings"
								}
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "dataset_binario_origem",
								"type": "DatasetReference",
								"parameters": {
									"container": "susbecantt",
									"folder": "SIASUA"
								}
							}
						],
						"outputs": [
							{
								"referenceName": "dataset_binario_destino",
								"type": "DatasetReference",
								"parameters": {
									"container": "bronze",
									"folder": "SIASUS"
								}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"filename": {
						"type": "string"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/dataset_binario_origem')]",
				"[concat(variables('factoryId'), '/datasets/dataset_binario_destino')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/bronze_hospitalar')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_hospitalar",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "BinarySource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"wildcardFolderPath": "SIHSUS",
									"wildcardFileName": "*",
									"deleteFilesAfterCompletion": false
								},
								"formatSettings": {
									"type": "BinaryReadSettings"
								}
							},
							"sink": {
								"type": "BinarySink",
								"storeSettings": {
									"type": "AzureBlobFSWriteSettings"
								}
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "dataset_binario_origem",
								"type": "DatasetReference",
								"parameters": {
									"container": "susbecantt",
									"folder": "SIHSUS"
								}
							}
						],
						"outputs": [
							{
								"referenceName": "dataset_binario_destino",
								"type": "DatasetReference",
								"parameters": {
									"container": "bronze",
									"folder": "SIHSUS"
								}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"filename": {
						"type": "string"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/dataset_binario_origem')]",
				"[concat(variables('factoryId'), '/datasets/dataset_binario_destino')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/trigger_ambulatorio')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "bronze_ambulatorio",
							"type": "PipelineReference"
						},
						"parameters": {
							"filename": "[parameters('trigger_ambulatorio_properties_bronze_ambulatorio_parameters_filename')]"
						}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/susbecantt/blobs/SIASUS",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('trigger_ambulatorio_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/bronze_ambulatorio')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/trigger_hospitalar')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "notebook_bronze",
							"type": "PipelineReference"
						},
						"parameters": {
							"filename": "[parameters('trigger_hospitalar_properties_notebook_bronze_parameters_filename')]"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "bronze_hospitalar",
							"type": "PipelineReference"
						},
						"parameters": {
							"filename": "[parameters('trigger_hospitalar_properties_bronze_hospitalar_parameters_filename')]"
						}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/susbecantt/blobs/SIHSUS",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('trigger_hospitalar_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/notebook_bronze')]",
				"[concat(variables('factoryId'), '/pipelines/bronze_hospitalar')]"
			]
		}
	]
}